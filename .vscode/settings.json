{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.files.exclude": [
    "**/.git/**",
    "**/node_modules/**",
    "**/.hg/**"
  ],
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  }
}
